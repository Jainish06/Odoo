<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="product_template_update_id" model="ir.ui.view">
        <field name="name">product.inherit</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//button[@name='action_open_label_layout']" position="after">
                <button name="action_update_qty1" type="object" string="Update Loc Qty"/>
            </xpath>
        </field>
    </record>
</odoo>
